{
    "name": "becklyn/static-roles-bundle",
    "type": "symfony-bundle",
    "description": "This bundle provides a simple way to define all available roles and their hierarchy in your configuration",
    "keywords": [
        "security",
        "symfony"
    ],
    "homepage": "https://github.com/Becklyn/BecklynStaticRolesBundle",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/config": "^6.1 || ^5.0",
        "symfony/dependency-injection": "^6.1 || ^5.0",
        "symfony/form": "^6.1 || ^5.0",
        "symfony/http-kernel": "^6.1 || ^5.0",
        "symfony/options-resolver": "^6.1 || ^5.0",
        "symfony/security-core": "^6.1 || ^5.0",
        "twig/twig": "^2.10 || ^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev",
            "dev-next": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Becklyn\\StaticRolesBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Becklyn\\StaticRolesBundle\\": "tests"
        }
    }
}
