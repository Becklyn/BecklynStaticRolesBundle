{
    "name": "becklyn/static-roles-bundle",
    "description": "This bundle provides a simple way to define all available roles and their hierarchy in your configuration",
    "keywords": ["security", "symfony"],
    "license": "BSD-3-Clause",
    "type": "symfony-bundle",
    "homepage": "https://github.com/Becklyn/BecklynStaticRolesBundle",
    "authors": [
        {
            "name": "Becklyn Studios",
            "homepage": "https://becklyn.com/"
        }
    ],
    "require": {
        "php": "^7.2",
        "symfony/config": "^5.0",
        "symfony/security-core": "^4.4 || ^5.0",
        "symfony/http-kernel": "^4.4 || ^5.0",
        "symfony/dependency-injection": "^5.0",
        "twig/twig": "^3.0",
        "symfony/options-resolver": "^5.0",
        "symfony/form": "^5.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.0"
    },
    "autoload": {
        "psr-4": {"Becklyn\\StaticRolesBundle\\": "src"}
    }
}
